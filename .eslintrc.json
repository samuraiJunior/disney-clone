{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-console": "warn",
    "prefer-const": "warn",
    "comma-dangle": [
      "warn",
      "always-multiline"
    ],
    "import/order": [
      "warn",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "object",
          "type"
        ]
      }
    ]
  }
}